#parameters from MCPB.py for Acetonitrile (resname AN1) to Zinc with Zinc Amber Force Field
addAtomTypes {
        { "M1"  "Zn" "sp3" }
        { "Y1"  "N" "sp3" }
        { "Y2"  "N" "sp3" }
        { "Y3"  "N" "sp3" }
        { "Y4"  "N" "sp3" }
}

#HX episilon protonated his
#HY delta protonated
#had to be renamed because of naming conflict with rotamer library
HX1 = loadmol2 ../../inp/HX1.mol2
HY1 = loadmol2 ../../inp/HY1.mol2
HY2 = loadmol2 ../../inp/HY2.mol2
ZN1 = loadmol2 ../../inp/ZN1.mol2
AN1 = loadmol2 ../../inp/AN1.mol2
AZI = loadmol2 ../../inp/AZI.mol2
loadamberparams ../../inp/AZI.frcmod
loadamberparams ../../inp/ACN.frcmod
loadamberparams frcmod.ions1lm_126_tip3p
loadamberparams ../../inp/GB1AZI_mcpbpy.frcmod

#necessary for igb8
set default PBRadii mbondi3

# load pdb twice, in one we have to delete the ligand to make the right parameter file for MMPBSA
mol = loadpdb mutated_ter.pdb
receptor = loadpdb mutated_ter.pdb
remove receptor receptor.59

# bond statements as required by ZAFF because of unusual residues
bond mol.5.ND1 mol.57.ZN
bond mol.16.NE2 mol.57.ZN
bond mol.18.NE2 mol.57.ZN
bond mol.57.ZN mol.58.N
bond mol.4.C mol.5.N
bond mol.5.C mol.6.N
bond mol.15.C mol.16.N
bond mol.16.C mol.17.N
bond mol.17.C mol.18.N
bond mol.18.C mol.19.N


bond receptor.5.ND1 receptor.57.ZN
bond receptor.16.NE2 receptor.57.ZN
bond receptor.18.NE2 receptor.57.ZN
bond receptor.57.ZN receptor.58.N
bond receptor.4.C receptor.5.N
bond receptor.5.C receptor.6.N
bond receptor.15.C receptor.16.N
bond receptor.16.C receptor.17.N
bond receptor.17.C receptor.18.N
bond receptor.18.C receptor.19.N


#save dry files and dry parameter files for MMPBSA
savepdb mol GB1AZI_dry.pdb
saveamberparm mol dry_complex.prmtop dry_complex.inpcrd
saveAmberParm receptor receptor.prmtop receptor.crd
saveAmberParm AZI ligand.prmtop ligand.crd


# solvate and neutralize for minimization
solvatebox mol TIP3PBOX 10.0
addions mol Na+ 0
addions mol Cl- 0
savepdb mol GB1AZI_solv.pdb
saveamberparm mol GB1AZI_solv.prmtop GB1AZI_solv.inpcrd
quit
 
